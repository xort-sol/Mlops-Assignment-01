{
    "version": 2,
    "builds": [
      {
        "src": "api/flaskRoute.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/flaskRoute.py"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
}